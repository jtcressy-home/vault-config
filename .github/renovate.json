{
  "text": {
    "matchFiles": [
      "path/to/your/textfile.txt"
    ],
    "matchStrings": [
      "VERSION_STRING"
    ],
    "versioning": {
      "major": "x",
      "minor": "y",
      "patch": "z",
      "source": "github-releases",
      "githubRelease": {
        "repo": "my-github-username/my-github-repo"
      }
    },
    "branchPrefix": "renovate/",
    "branchName": "renovate/VERSION_STRING"
  },
  "schedule": {
    "interval": "daily",
    "timezone": "UTC"
  }
}
