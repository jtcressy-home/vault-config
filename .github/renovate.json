{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "packageNames": ["vault"],
      "depTypeList": ["text"],
      "updateTypes": ["versioning"],
      "matchStrings": ["VERSION_STRING"],
      "fileMatch": [".vault-version"],
      "versioning": {
        "major": "x",
        "minor": "y",
        "patch": "z"
      },
      "schedule": {
        "interval": "daily"
      },
      "repository": "hashicorp/vault",
      "branchName": "renovate/VERSION_STRING"
    }
  ]
}
